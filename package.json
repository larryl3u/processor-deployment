{
  "name": "movement-fullnode",
  "version": "1.0.0",
  "main": "dist/main.js",
  "types": "dist/types/main.d.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "help": "npx ts-node main.ts --help",
    "clean": "rm -rf dist/ cdktf.out/ .gen/",
    "get": "cdktf get",
    "compile": "tsc --build",
    "watch": "tsc --build -w",
    "build": "pnpm run clean && pnpm run get && pnpm run compile && pnpm run k8s:synth:testnet",
    
    "k8s:synth:testnet": "cdktf synth --target=kubernetes_testnet",
    "k8s:synth:devnet": "cdktf synth --target=kubernetes_devnet", 
    "k8s:synth:mainnet": "cdktf synth --target=kubernetes_mainnet",
    
    "k8s:deploy:testnet": "cdktf deploy --target=kubernetes_testnet",
    "k8s:deploy:devnet": "cdktf deploy --target=kubernetes_devnet",
    "k8s:deploy:mainnet": "cdktf deploy --target=kubernetes_mainnet",
    
    "k8s:destroy:testnet": "cdktf destroy --target=kubernetes_testnet",
    "k8s:destroy:devnet": "cdktf destroy --target=kubernetes_devnet", 
    "k8s:destroy:mainnet": "cdktf destroy --target=kubernetes_mainnet",
    
    "k8s:diff:testnet": "cdktf diff --target=kubernetes_testnet",
    "k8s:diff:devnet": "cdktf diff --target=kubernetes_devnet",
    "k8s:diff:mainnet": "cdktf diff --target=kubernetes_mainnet"
  },
  "dependencies": {
    "cdktf": "^0.21.0",
    "constructs": "^10.4.2",
    "@cdktf/provider-kubernetes": "^12.1.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^14",
    "@types/yargs": "^17.0.33",
    "ts-node": "^10",
    "typescript": "^4.9.5"
  }
}